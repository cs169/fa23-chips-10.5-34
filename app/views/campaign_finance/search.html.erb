<div class="container mt-5">
  <div class="row justify-content-center">
    <div class="col-md-12">
      <% if @data.present? %>
        <table class="table">
          <thead>
            <tr>
              <th>Candidate Names</th>
            </tr>
          </thead>
          <tbody>
            <% @data.each do |candidate| %>
              <tr>
                <td><%= candidate['name'] %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      <% else %>
        <p>No candidates found.</p>
      <% end %>
    </div>
  </div>
</div>
